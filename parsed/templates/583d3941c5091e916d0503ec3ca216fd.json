{"components/products/description-tabs":"<ul class=\"tabs\" data-tab>\n    <li class=\"tab is-active\">\n        <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\n    </li>\n    {{#if product.warranty}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-warranty\">Documentation</a>\n        </li>\n    {{/if}}\n    {{#if product.videos.list.length}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-video\">{{lang 'products.videos.header'}}</a>\n        </li>\n    {{/if}}\n</ul>\n<div class=\"tabs-contents\">\n    <div class=\"tab-content is-active\" id=\"tab-description\">\n        {{{product.description}}}\n        {{{snippet 'product_description'}}}\n        <div class=\"custom_field_wrap\">\n            {{#each product.custom_fields}}\n                <div class=\"product_custom_field_box\">\n                    <div class=\"product_custom_fields productView-info-name\">{{name}}</div>\n                    <div class=\"product_custom_fields productView-info-value {{name}}_list\">{{{value}}}</div>\n                </div>\n            {{/each}}\n        </div>\n        <div class=\"DescriptionContent_Outer\">\n            <div id=\"icon-list\" class=\"iconSet\"></div>\n            <div id=\"features-list\" class=\"featureSet\"></div>\n        </div>\n    </div>\n    <div class=\"tab-content\" id=\"tab-video\">\n   {{#if product.videos.list.length}}\n        {{> components/products/videos product.videos}}\n    {{/if}}\n    </div>\n   {{#if product.warranty}}\n       <div class=\"tab-content\" id=\"tab-warranty\">\n           {{{product.warranty}}}\n       </div>\n   {{/if}}\n\n</div>\n","components/products/videos":"<section class=\"toggle videoGallery\" data-video-gallery>\n    <!--<h4 class=\"toggle-title\">\n        {{lang 'products.videos.header'}}\n        <a href=\"#videoGallery-content\" class=\"toggleLink is-open\" data-collapsible>\n            <span class=\"toggleLink-text toggleLink-text--on\">\n                {{lang 'products.videos.hide'}}\n            </span>\n            <span class=\"toggleLink-text toggleLink-text--off\">\n                {{lang 'products.videos.show'}}\n            </span>\n        </a>\n    </h4>-->\n\n    <div class=\"toggle-content is-open\" id=\"videoGallery-content\">\n        <div class=\"videoGallery-main\">\n            <iframe\n                id=\"player\"\n                type=\"text/html\"\n                width=\"640\"\n                height=\"390\"\n                frameborder=\"0\"\n                webkitAllowFullScreen\n                mozallowfullscreen\n                allowFullScreen\n                src=\"//www.youtube.com/embed/{{this.featured.id}}\"\n                data-video-player>\n            </iframe>\n        </div>\n        <ul class=\"videoGallery-list\">\n        {{#each this.list}}\n            <li class=\"videoGallery-item\">\n                <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\n                    <div class=\"video-figure\">\n                        <img src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\n                    </div>\n                    <div class=\"video-body\">\n                        <h5 class=\"video-title\">{{title_long}}</h5>\n                        <p class=\"video-description\">{{description_short}}</p>\n                    </div>\n                </a>\n            </li>\n        {{/each}}\n        </ul>\n    </div>\n\n</section>\n"}
